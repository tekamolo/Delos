{
    "name": "delos/framework",
    "description": "PHP Framework",
    "keywords": [
        "framework",
        "delos"
    ],
    "type": "project",
    "autoload": {
        "psr-4": {
            "Delos\\": "framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8",
        "ext-simplexml": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/console": "^6.0",
        "josegonzalez/dotenv": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0",
        "dg/bypass-finals": "^1.3",
        "illuminate/database": "^9.5",
        "doctrine/dbal": "^3.3",
        "fakerphp/faker": "^1.19",
        "twig/twig": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alfonso Fernandez Ocampo",
            "email": "fernandez1980@gmail.com"
        }
    ],
	"minimum-stability": "stable"
